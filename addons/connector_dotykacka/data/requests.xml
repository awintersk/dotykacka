        <field name="name">Refresh Token</field>
        <field name="provider" ref="connector_dotykacka.provider_dotykacka_get_accesstoken"/>
        <field name="method">POST</field>
        <field name="url_path">/v2/signin/token</field>
        <field name="content_type">application/json</field>
    </record>
    <record model="api_manager.request" id="api_request_dotykacka_create_product">
